0. cd tests, pytest, cd devtests, bash devtests.sh
1. update version in: setup.py, setup.cfg
2. git add .
3. git commit -m 'commit message'
4. git tag 0.0.0
5. git push origin --tags
6. rm -rf build dist *.egg-info
7. pip install --upgrade build twine
8. python -m build
9. python -m twine upload (--repository testpypi) dist/*
10. python -m venv ivenv
11. source ivenv/bin/activate
12. pip install (--extra-index-url https://test.pypi.org/simple/) wsp-tools
